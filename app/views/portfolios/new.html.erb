


<div class="container">
  <div class="portnew row"><center>
    <br><br><br><br><br>
    <h1>Add a New Portfolio</h1>
    <br><br><br><br><br>

    <%= form_for Portfolio.new do |f| %>

    <%= f.label :title %>
    <%= f.text_field :title %> <br>

    <%= f.hidden_field :user_id, :value => current_user.id %>

    <%= f.submit %>
    <% end %>
  </div>


</div>
